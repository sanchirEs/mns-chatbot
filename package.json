{
  "name": "ai-pharmacy-chatbot-enterprise",
  "version": "1.0.0",
  "type": "module",
  "description": "Enterprise-grade AI chatbot for pharmacy operations with RAG, function calling, and advanced conversation management",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js --watch src",
    "build": "echo \"No build step required for Node.js\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "health-check": "node scripts/health-check.js",
    "cleanup": "node scripts/cleanup.js",
    "sync:products": "node scripts/sync-products.js",
    "sync:test": "node scripts/test-sync.js",
    "sync:status": "node -e \"import('./src/services/dataSyncService.js').then(m => m.DataSyncService.getSyncStatus()).then(console.log)\"",
    "migrate": "node scripts/migrate-to-three-tier.js",
    "sync:full": "node scripts/sync-products.js --max-products=1000",
    "sync:quick": "node -e \"import('./src/services/dataSyncService.js').then(m => m.DataSyncService.quickStockSync())\"",
    "cache:clear": "node -e \"import('./src/services/dataSyncService.js').then(m => m.DataSyncService.clearAllCaches())\"",
    "embeddings:generate": "node scripts/generate-embeddings.js",
    "embeddings:all": "node scripts/generate-embeddings.js --all"
  },
  "keywords": [
    "ai",
    "chatbot",
    "pharmacy",
    "openai",
    "rag",
    "function-calling",
    "enterprise",
    "healthcare",
    "supabase",
    "vector-database"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.20.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-pharmacy-chatbot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-pharmacy-chatbot/issues"
  },
  "homepage": "https://github.com/yourusername/ai-pharmacy-chatbot#readme"
}
